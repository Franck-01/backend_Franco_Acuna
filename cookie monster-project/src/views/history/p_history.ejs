<% if (files.length>0) { %>
    <% files.forEach(forms => { %>
        <div>
            <table>
                <tr>
                    <td>
                        <p>
                            <%=forms.name  %>
                        </p>
                        <p>
                            <%= forms.model  %>
                        </p>
                        <p>
                            <%= forms.faction  %>
                        </p>
                    </td>
                    <td><img src=<%=forms.img %>></td>
                </tr>
            </table>


        </div>
        <% }) %>
            <% } else { %>
                <p>Por ahora, no hay informacion disponible</p>
                <% } %>