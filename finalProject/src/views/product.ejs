<h1>Productos disponibles</h1>
<% if (products.length>0) { %>
<% products.forEach(product =>{ %>
            <tbody>
                <tr>
                  <th scope="row"><%=product.id%></th>
                  <td><%=product.name %></td>
                  <td><%=product.description %></td>
                  <td><%=product.price %></td>
</tr>
            </tbody>
        <% }) %> 
        <% } else { %>
        <p>No hay usuarios por mostrar</p>
<% } %>